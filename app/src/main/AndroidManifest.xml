<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doctor_appointment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/doctor_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".user_personal_details"></activity>
        <activity android:name=".doctorDashboard" />
        <activity android:name=".userDashboard" />
        <activity android:name=".userregister" />
        <activity android:name=".userlogin" />
        <activity android:name=".doctorregister" />
        <activity android:name=".doctorlogin" />
        <activity android:name=".askdoctor" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>